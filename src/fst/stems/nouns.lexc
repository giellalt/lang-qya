    nostalë:nostale{+}                      NounSuffix-E    "species, kind" ;
    oio                                     NounSuffix      "endless period" ;
    orë:or{stemi2e}                         NounSuffix      "grain" ;
    órë:óre                                 NounSuffix-E    "heart, inner mind (tengwa 21)" ;
    órë:óre                                 NounSuffix-E    "rising" ;
    oron:oron{stemt20}                      NounSuffix-Cons "mountain" ;
    quenta                                  NounSuffix      "tale, narrative, story" ;
    rocco                                   NounSuffix      "horse" ;
    ranco:ranc{stemu2o}                     NounSuffix-Cons "arm" ;
    sar:sar{stemd20}                        NounSuffix-Cons "pebble, stone" ;
    sar:sar{stemn20}                        NounSuffix-Cons "pebble, stone" ;
    súrë:súr{stemi2e}                       NounSuffix      "wind, breeze" ;
    talan:tala{stemm2n}                     NounSuffix-Cons "floor" ;
    tári                                    NounSuffix      "queen" ;
    tasar:taþar                             NounSuffix-Cons "willow" ;
    tasarë:taþare{+}                        NounSuffix-E    "willow" ;
    tië:tie                                 NounSuffix      "path" ;
    tiuco                                   NounSuffix      "thigh" ;
    tyalië:tyalie{+}                        NounSuffix      "sport, play, game" ;
    úmë:úme                                 NounSuffix-E    "throng, great collection" ;
