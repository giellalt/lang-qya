    vendë:{w}ende                           NounSuffix-E    "maiden, virgin" ;

    ! exceptions

    ! special cases of hína
    hína+N:[+Voc]hina             NounNoCase  "child (vocative)" ;
    hína+N+PxSg1:[+Voc]hinanya    NounNoCase  "my child (vocative)" ;
    hína+N+PxSg1:[+Voc]hinya      NounNoCase  "my child (vocative)" ;
    ! special cases of yondo
    yondo+N:[+Voc]yondo           NounNoCase  "son (vocative)" ;
    yondo+N+PxSg1:[+Voc]yondonya  NounNoCase  "my son (vocative)" ;
    yondo+N+PxSg1:[+Voc]yonya     NounNoCase  "my son (vocative)" ;

