!! Nouns
!  -----
!! Nouns in the Quenya language are people, places, or things.

LEXICON Nouns
    ! Lesson 2
    anar                                    NounSuffix-Cons "(the) sun" ;
    isil:iþil                               NounSuffix-Cons "(the) moon" ;
    elda                                    NounSuffix      "elf" ;
    lië:lie{+}                              NounSuffix      "people, race" ;
    vendë:{w}ende                           NounSuffix-E    "maiden, virgin" ;
    rocco                                   NounSuffix      "horse" ;
    aran                                    NounSuffix-Cons "king" ;
    tári                                    NounSuffix      "queen" ;
    tasar:taþar                             NounSuffix-Cons "willow" ;
    ! tasarë:taþare{+}                        NounSuffix-E    "willow" ;
    ! Lesson 3
    hen:hen{stemd20}                        NounSuffix-Cons "eye" ;
    ranco:ranc{stemu2o}                     NounSuffix-Cons "arm" ;
    ando                                    NounSuffix      "gate"  ;
    cirya                                   NounSuffix      "ship" ;
    aiwë:aiwe                               NounSuffix-E    "bird" ;
    talan:tala{stemm2n}                     NounSuffix-Cons "floor" ;
    nér:n{steme2é}r                         NounSuffix-Cons "man (adult male)" ;
    nís:n{stemi2í}s{stems20}                NounSuffix-Cons "woman (adult female)" ;
    sar:sar{stemd20}                        NounSuffix-Cons "pebble, stone" ;
    ! sar:sar{stemn20}                        NounSuffix-Cons "pebble, stone" ;
    alda                                    NounSuffix      "tree" ;
    oron:oron{stemt20}                      NounSuffix-Cons "mountain" ;
    ! Assorted
    nénu                                    NounSuffix      "yellow water-lily" ;
    tyalië:tyalie{+}                        NounSuffix      "sport, play, game" ;
    coimas                                  NounSuffix-Cons "life-bread, lembas" ;
    manar                                   NounSuffix-Cons "doom, fate, fortune" ;
    estel                                   NounSuffix-Cons "trust, hope" ;
    lirit                                   NounSuffix-Cons "poem" ;
    ango:ang{stemu2o}                       NounSuffix-Cons "snake" ;
    súrë:súr{stemi2e}                       NounSuffix      "wind, breeze" ;
    lassë:lasse                             NounSuffix-E    "leaf" ;
    
    anta                                    NounSuffix      "face" ;
    atan                                    NounSuffix-Cons "man (as opposed to elf)" ;
    aurë:aure                               NounSuffix-E    "sunlight, day" ;
    cálë:cále                               NounSuffix-E    "light" ;
    cemen                                   NounSuffix-Cons "earth, ground, soil" ;
    cemendë:cemende                         NounSuffix-E    "earth, ground, soil" ;
    ëar:ear                                 NounSuffix-Cons "sea" ;
    Eru                                     NounSuffix      "the One, God" ;
    írë:íre                                 NounSuffix-E    "desire" ;
    ilya                                    NounSuffix      "all, the whole, each, every" ;
    ista                                    NounSuffix      "knowledge" ;
    lúmë:lúme                               NounSuffix-E    "time, hour" ;
    lúmë:lúme                               NounSuffix-E    "darkness" ;
    menel                                   NounSuffix-Cons "the heavens, firmament" ;
    metta                                   NounSuffix      "end" ;
    ne                                      NounSuffix-E    "scent" ;
    nostalë:nostale                         NounSuffix-E    "species, kind" ;
    orë:or{stemi2e}                         NounSuffix      "grain" ;
    órë:óre                                 NounSuffix-E    "heart, inner mind (tengwa 21)" ;
    órë:óre                                 NounSuffix-E    "rising" ;
    úmë:úme                                 NounSuffix-E    "throng, great collection" ;
    vanda                                   NounSuffix      "oath, pledge" ;
    Yehó{w}a                                NounSuffix      "Jehova" ;

    ! exceptions

    ! special cases of hína
    @U.Case.Voc@hína+N:@U.Case.Voc@hina             NounNoCase  "child (vocative)" ;
    @U.Case.Voc@hína+N+PxSg1:@U.Case.Voc@hinanya    NounNoCase  "my child (vocative)" ;
    @U.Case.Voc@hína+N+PxSg1:@U.Case.Voc@hinya      NounNoCase  "my child (vocative)" ;
    ! special cases of yondo
    @U.Case.Voc@yondo+N:@U.Case.Voc@yondo           NounNoCase  "son (vocative)" ;
    @U.Case.Voc@yondo+N+PxSg1:@U.Case.Voc@yondonya  NounNoCase  "my son (vocative)" ;
    @U.Case.Voc@yondo+N+PxSg1:@U.Case.Voc@yonya     NounNoCase  "my son (vocative)" ;

! vim: set ft=xfst-lexc:
